    <entry>
      <name>Transformação 1 Exames</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>drop table if exists tmp_001_fapesp_exames;
create table tmp_001_fapesp_exames as
SELECT
	ID_PACIENTE as paciente_key,
	ID_ATENDIMENTO as atendimento_key,
	DT_COLETA as data_key,
	DE_EXAME as exame_key,
	DE_ANALITO as analito_key,
	DE_ORIGEM as origem_key,
	case
		when DE_RESULTADO regexp '([\-\+]?[0-9]+([,]{1}[0-9]+)?)'
			then cast(replace(DE_RESULTADO, ',', '.') as FLOAT)
		else null
	end as resultado_numerico,
	case
		when DE_RESULTADO regexp '([\-\+]?[0-9]+([,]{1}[0-9]+)?)'
			then NULL
		else DE_RESULTADO
	end as resultado_textual,
	CD_UNIDADE as unidade_medida,
	case
		when DE_VALOR_REFERENCIA = '' then null
		when DE_VALOR_REFERENCIA regexp '([\-\+]?[0-9]+([,]{1}[0-9]+)?).[\-].([\-\+]?[0-9]+([,]{1}[0-9]+)?)' 
			then cast(TRIM(substring_index(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), '-', 1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp '([\-\+]?[0-9]+([,]{1}[0-9]+)?).[a].([\-\+]?[0-9]+([,]{1}[0-9]+)?)' 
			then cast(TRIM(substring_index(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), 'a', 1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp 'Até.([0-9]+([,\.]{1}[0-9]+)?)' then null
		when DE_VALOR_REFERENCIA regexp 'Até..([0-9]+([,\.]{1}[0-9]+)?)' then null
		when DE_VALOR_REFERENCIA regexp 'inferior a ([0-9]+([,]{1}[0-9]+)?)' then null
		when DE_VALOR_REFERENCIA regexp 'Superior a ([0-9]+([,]{1}[0-9]+)?)' 
			then cast(trim(substring_index(replace(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), 'Superior a ', ''),' ', 1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp 'menor que ([0-9]+([,]{1}[0-9]+)?)' then null
		when DE_VALOR_REFERENCIA regexp 'maior que ([0-9]+([,]{1}[0-9]+)?)'
			then cast(trim(substring_index(replace(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), 'maior que ', ''), ' ', 1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp 'maior que a ([0-9]+([,]{1}[0-9]+)?)'
			then cast(trim(substring_index(replace(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), 'maior que a ', ''), ' ', 1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp '&lt; ([0-9]+([,]{1}[0-9]+)?)' then null
		when DE_VALOR_REFERENCIA regexp '> ([0-9]+([,]{1}[0-9]+)?)'
			then cast(trim(substring_index(replace(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), '> ', ''), ' ', 1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp 'Maior ou igual a ([0-9]+([,]{1}[0-9]+)?)'
			then cast(trim(substring_index(replace(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), 'Maior ou igual a ', ''), ' ', 1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp 'Menor ou igual a ([0-9]+([,]{1}[0-9]+)?)' then null
		when DE_VALOR_REFERENCIA regexp 'Acima de ([0-9]+([,]{1}[0-9]+)?)'
			then cast(trim(substring_index(replace(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), 'Acima de ', ''), ' ', 1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp 'Inferior ou igual a ([0-9]+([,]{1}[0-9]+)?)' then null
	end as limite_inferior,
	case
		when DE_VALOR_REFERENCIA = '' then null
		when DE_VALOR_REFERENCIA regexp '([\-\+]?[0-9]+([,]{1}[0-9]+)?).[\-].([\-\+]?[0-9]+([,]{1}[0-9]+)?)' 
			then cast(TRIM(substring_index(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), '-', -1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp '([\-\+]?[0-9]+([,]{1}[0-9]+)?).[a].([\-\+]?[0-9]+([,]{1}[0-9]+)?)' 
			then cast(TRIM(substring_index(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), 'a', -1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp 'Até.([0-9]+([,\.]{1}[0-9]+)?)'
			then cast(trim(substring_index(replace(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), 'Até ', ''),' ', 1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp 'Até..([0-9]+([,\.]{1}[0-9]+)?)'
			then cast(trim(substring_index(replace(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), 'Até  ', ''),' ', 1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp 'inferior a ([0-9]+([,]{1}[0-9]+)?)'
			then cast(trim(substring_index(replace(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), 'inferior a ', ''),' ', 1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp 'Superior a ([0-9]+([,]{1}[0-9]+)?)' then null
		when DE_VALOR_REFERENCIA regexp 'menor que ([0-9]+([,]{1}[0-9]+)?)'
			then cast(trim(substring_index(replace(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), 'menor que ', ''),' ', 1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp 'maior que ([0-9]+([,]{1}[0-9]+)?)' then null
		when DE_VALOR_REFERENCIA regexp 'maior que a ([0-9]+([,]{1}[0-9]+)?)' then null
		when DE_VALOR_REFERENCIA regexp '&lt; ([0-9]+([,]{1}[0-9]+)?)'
			then cast(trim(substring_index(replace(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), '&lt; ', ''),' ', 1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp '> ([0-9]+([,]{1}[0-9]+)?)' then null
		when DE_VALOR_REFERENCIA regexp 'Maior ou igual a ([0-9]+([,]{1}[0-9]+)?)' then null
		when DE_VALOR_REFERENCIA regexp 'Menor ou igual a ([0-9]+([,]{1}[0-9]+)?)'
			then cast(trim(substring_index(replace(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), 'Menor ou igual a ', ''),' ', 1)) as FLOAT)
		when DE_VALOR_REFERENCIA regexp 'Acima de ([0-9]+([,]{1}[0-9]+)?)' then null
		when DE_VALOR_REFERENCIA regexp 'Inferior ou igual a ([0-9]+([,]{1}[0-9]+)?)'
			then cast(trim(substring_index(replace(replace(replace(DE_VALOR_REFERENCIA, '.',''), ',', '.'), 'Inferior ou igual a ', ''),' ', 1)) as FLOAT)
	end as limite_superior,
	DE_VALOR_REFERENCIA as referencia
FROM (
	SELECT * FROM stg_001_fapesp_exames_sirio_libanes
	--UNION ALL SELECT * FROM stg_001_fapesp_exames_fleury
	--UNION ALL SELECT * FROM stg_001_fapesp_exames_einstein
) EXAMES 
WHERE
	1=1
;</sql>
