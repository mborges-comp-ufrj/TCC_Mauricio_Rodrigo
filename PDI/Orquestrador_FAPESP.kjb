      <sql>INSERT INTO stg_002_fapesp_exames   
SELECT
	ex_todos.exame_paciente_key            AS exame_paciente_key, 
	ex_todos.exame_atendimento_key         AS exame_atendimento_key,
	ex_todos.exame_data_coleta             AS exame_data_coleta,
	ex_todos.exame_origem                  AS exame_origem,
	ex_todos.exame_exame                   AS exame_exame,
	ex_todos.exame_analito                 AS exame_analito,
	ex_todos.exame_resultado               AS exame_resultado,
	ex_todos.exame_unidade_medida          AS exame_unidade_medida,
	ex_todos.exame_valor_referencia        AS exame_valor_referencia,
	ex_todos.exame_hospital                AS exame_hospital,
	ex_covid.exame_metodo_deteccao_covid   AS exame_metodo_deteccao_covid
FROM
	stg_002_fapesp_exames_total ex_todos
	INNER JOIN (SELECT DISTINCT exame_paciente_key, exame_metodo_deteccao_covid FROM stg_002_fapesp_exames_covid) ex_covid
		ON ex_todos.exame_paciente_key = ex_covid.exame_paciente_key
;</sql>
