    <entry>
      <name>Transformação 2 Exames 2</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>insert into rel_001_fapesp_exame (
	paciente_key,
	atendimento_key,
	data_key,
	exame_key,
	analito_key,
	origem_key,
	rel_001_fapesp_paciente_paciente_id,
	rel_001_fapesp_desfecho_desfecho_id,
	resultado_numerico,
	limite_inferior,
	limite_superior,
	resultado_normalizado,
	resultado_textual,
	referencia,
	origem,
	dia_atendimento,
	horario_insercao
) select
	insumo.paciente_key,
	insumo.atendimento_key,
	insumo.data_key,
	insumo.exame_key,
	insumo.analito_key,
	insumo.origem_key,
	paciente.paciente_id as rel_001_fapesp_paciente_paciente_id,
	desfecho.desfecho_id as rel_001_fapesp_desfecho_desfecho_id,
	insumo.resultado_numerico,
	insumo.limite_inferior,
	insumo.limite_superior,
	case
		when insumo.resultado_numerico is not null and insumo.limite_inferior is not null and insumo.limite_superior is not null
			then (insumo.resultado_numerico - insumo.limite_inferior)/(insumo.limite_superior-insumo.limite_inferior)
		when insumo.resultado_numerico is not null and insumo.limite_inferior is not null and insumo.limite_superior is null
			then (insumo.resultado_numerico-insumo.limite_inferior)/(resultado_maximo - insumo.limite_inferior)
		when insumo.resultado_numerico is not null and insumo.limite_inferior is null and insumo.limite_superior is not null
			then (insumo.resultado_numerico-resultado_minimo)/(insumo.limite_superior - resultado_minimo)
		when insumo.resultado_numerico is not null and insumo.limite_inferior is null and insumo.limite_superior is null
			then (insumo.resultado_numerico-resultado_minimo)/(resultado_maximo - resultado_minimo)
		else null
	end as resultado_normalizado,
	insumo.resultado_textual,
	insumo.referencia,
	'Hospital Sírio-Libanês' as origem,
	datediff(insumo.data_key, min_data.data_inicial) as dia_atendimento,
	NOW() as horario_insercao
from
	tmp_001_fapesp_exames insumo
	left join (
		select 
			exame_key ex, 
			analito_key an, 
			MAX(resultado_numerico) resultado_maximo, 
			MIN(resultado_numerico) resultado_minimo
		from	
			tmp_001_fapesp_exames insumo
		group by
			exame_key,
			analito_key
	) min_max
		on insumo.exame_key = min_max.ex
		and insumo.analito_key = min_max.an
	left join (
		select 
			paciente_key pac,
			atendimento_key atend, 
			MIN(data_key) data_inicial
		from	
			tmp_001_fapesp_exames insumo
		group by
			paciente_key,
			atendimento_key
	) min_data
		on insumo.paciente_key = min_data.pac
		and insumo.atendimento_key = min_data.atend
	left join rel_001_fapesp_paciente paciente
		on insumo.paciente_key = paciente.key
	left join rel_001_fapesp_desfecho desfecho
		on insumo.atendimento_key = desfecho.atendimento_key
		and insumo.paciente_key = desfecho.paciente_key
	left join rel_001_fapesp_exame rel
		on insumo.paciente_key = rel.paciente_key
		and insumo.atendimento_key = rel.atendimento_key
		and insumo.data_key = rel.data_key
		and insumo.exame_key = rel.exame_key
		and insumo.analito_key = rel.analito_key
		and insumo.origem_key = rel.origem_key
where
	rel.paciente_key is null
;</sql>
